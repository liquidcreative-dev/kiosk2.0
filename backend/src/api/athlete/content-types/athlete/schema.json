{
  "kind": "collectionType",
  "collectionName": "athletes",
  "info": {
    "singularName": "athlete",
    "pluralName": "athletes",
    "displayName": "Athlete",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "class": {
      "type": "string",
      "default": "null"
    },
    "prevSchool": {
      "type": "string",
      "default": "null"
    },
    "highschool": {
      "type": "string",
      "default": "null"
    },
    "major": {
      "type": "string",
      "required": false,
      "default": "null"
    },
    "stats": {
      "type": "text",
      "default": "null"
    },
    "finalist": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "award": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::award.award",
      "inversedBy": "athletes"
    },
    "years": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::year.year",
      "inversedBy": "athletes"
    },
    "jerseyNum": {
      "type": "integer",
      "default": 0
    },
    "weight": {
      "type": "integer"
    },
    "sport": {
      "type": "enumeration",
      "enum": [
        "football",
        "basketball (M)",
        "baseball",
        "basketball (W)"
      ],
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "QB",
        "RB",
        "WR",
        "TE",
        "OL",
        "DL",
        "LB",
        "DB",
        "K",
        "LS",
        "PW",
        "SF",
        "PG",
        "SG",
        "PF",
        "C",
        "OF",
        "IF",
        "P",
        "G",
        "F"
      ],
      "required": true
    },
    "hometown": {
      "type": "string",
      "default": "null"
    },
    "school": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::school.school"
    },
    "positionFullName": {
      "type": "enumeration",
      "enum": [
        "Quarterback ",
        "Wide Receiver",
        "Running Back",
        "Tight End",
        "Linebacker",
        "Defensive Lineman",
        "Defensive Back",
        "Kicker",
        "Punter",
        "Offensive Lineman",
        "Center",
        "Power Forward",
        "Point Guard",
        "Shooting Guard",
        "Shooting Forward",
        "Outfield",
        "Infield",
        "Pitcher",
        "Catcher",
        "Guard",
        "Forward"
      ],
      "required": true
    },
    "imageLink": {
      "type": "string"
    },
    "schoolName": {
      "type": "string"
    },
    "schoolLogo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "schoolLogoLink": {
      "type": "string"
    },
    "heightInFeet": {
      "type": "string",
      "default": "null"
    },
    "heightInInches": {
      "type": "string",
      "default": "null"
    },
    "ceremonyVideo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "highlightsVideo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
